# Application Configuration
NEXT_PUBLIC_APP_NAME="Flowise Converter"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_APP_DESCRIPTION="Frontend for Flowise to LangChain converter"

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_API_TIMEOUT=30000
NEXT_PUBLIC_API_RETRY_ATTEMPTS=3

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENV=development

# Database (if needed for API)
DATABASE_URL=postgresql://username:password@localhost:5432/tester_bot
DATABASE_POOL_SIZE=10

# Authentication (if implemented)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# External Services
OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_API_KEY=your-langchain-api-key
FLOWISE_API_URL=http://localhost:3000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Performance
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_PERFORMANCE_MONITORING=false

# Feature Flags
NEXT_PUBLIC_ENABLE_CHAT_TESTING=true
NEXT_PUBLIC_ENABLE_FLOW_CONVERSION=true
NEXT_PUBLIC_ENABLE_ADVANCED_ANALYTICS=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# Build Configuration
ANALYZE=false
BUNDLE_ANALYZE=false
NEXT_PUBLIC_BUILD_ID=
NEXT_PUBLIC_BUILD_TIME=

# Deployment
DEPLOY_ENV=development
HEALTH_CHECK_PORT=3000
HEALTH_CHECK_PATH=/api/health