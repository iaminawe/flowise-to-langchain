{
  "name": "tester-bot-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for Flowise to LangChain tester bot",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "nodemon server/index.js",
    "build": "next build",
    "start": "next start",
    "start:all": "concurrently \"npm start\" \"npm run start:server\"",
    "start:server": "node server/index.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prebuild": "npm run lint && npm run type-check",
    "postbuild": "npm run test:ci",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "deploy:build": "npm run clean && npm run build",
    "deploy:docker": "docker build -t tester-bot-frontend .",
    "deploy:docker:run": "docker run -p 3000:3000 tester-bot-frontend",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.0.0",
    "@codemirror/lang-python": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tanstack/react-query": "^5.8.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "codemirror": "^6.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.0",
    "react-select": "^5.7.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.5.0",
    "recharts": "^2.8.0",
    "swr": "^2.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
